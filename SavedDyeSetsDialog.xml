<GuiXml>
	<Controls>
		<TopLevelControl name="SaveSet" inherits="ZO_CustomDialogBase">
			<OnInitialized>
				SavedDyeSets_SetupSaveDialog(self)
			</OnInitialized>

			<Controls>
                <Label name="$(parent)Prompt" inherits="ZO_DialogContentAnchors" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_NORMAL" font="ZoFontGame"/>

                

                <Control name="$(parent)Cancel" inherits="ZO_CustomDialogButton"/>
                <Control name="$(parent)Save" inherits="ZO_CustomDialogButton"/>
            </Controls>
		</TopLevelControl>

		<TopLevelControl name="DeleteSet" inherits="ZO_CustomDialogBase">
			<OnInitialized>
				SavedDyeSets_SetupDeleteDialog(self)
			</OnInitialized>

			<Controls>
                <Label name="$(parent)Prompt" inherits="ZO_DialogContentAnchors" color="INTERFACE_COLOR_TYPE_TEXT_COLORS:INTERFACE_TEXT_COLOR_NORMAL" font="ZoFontGame"/>

                

                <Control name="$(parent)Cancel" inherits="ZO_CustomDialogButton"/>
                <Control name="$(parent)Delete" inherits="ZO_CustomDialogButton"/>
            </Controls>
		</TopLevelControl>
	</Controls>
</GuiXml>